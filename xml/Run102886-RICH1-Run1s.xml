<?xml version="1.0" encoding="ISO-8859-1" standalone="no" ?><!DOCTYPE DDDB SYSTEM "../../../DTD/structure.dtd"><DDDB>
<!--
  The Rich mirrors are made as spherical segments with the centre of
  the mirror around the local x axis. Rotation should be done around the
  centre of the mirror, which is at x=radius.
  The current radius is 2710*mm and is included in the pivot point.
  As the mirors have their local centre around x=radius, in order to change
  the CoC in y (global) a rotation around the local z is required.
  -->
<!-- Spherical mirrors -->
  <condition classID="6" name="SphMirror0_Align">
    <paramVector name="dPosXYZ" type="double">0*mm 0*mm 0*mm</paramVector>
    <paramVector name="dRotXYZ" type="double">   0.00000*mrad   -1.39316*mrad    1.02616*mrad</paramVector>
    <paramVector name="pivotXYZ" type="double">2710.0*mm 0*mm 0*mm</paramVector>
  </condition>
  <condition classID="6" name="SphMirror1_Align">
    <paramVector name="dPosXYZ" type="double">0*mm 0*mm 0*mm</paramVector>
    <paramVector name="dRotXYZ" type="double">   0.00000*mrad   -1.33593*mrad    2.61305*mrad</paramVector>
    <paramVector name="pivotXYZ" type="double">2710.0*mm 0*mm 0*mm</paramVector>
  </condition>
  <condition classID="6" name="SphMirror2_Align">
    <paramVector name="dPosXYZ" type="double">0*mm 0*mm 0*mm</paramVector>
    <paramVector name="dRotXYZ" type="double">   0.00000*mrad    0.35992*mrad   -0.74771*mrad</paramVector>
    <paramVector name="pivotXYZ" type="double">2710.0*mm 0*mm 0*mm</paramVector>
  </condition>
  <condition classID="6" name="SphMirror3_Align">
    <paramVector name="dPosXYZ" type="double">0*mm 0*mm 0*mm</paramVector>
    <paramVector name="dRotXYZ" type="double">   0.00000*mrad    0.58536*mrad   -2.01320*mrad</paramVector>
    <paramVector name="pivotXYZ" type="double">2710.0*mm 0*mm 0*mm</paramVector>
  </condition>
<!-- Secondary (flat) mirrors -->
